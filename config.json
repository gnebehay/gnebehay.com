{
  "locals": {
    "url": "http://localhost:8080",
    "name": "The Wintersmith's blog",
    "owner": "Someone",
    "description": "Ramblings of an immor(t)al demigod"
  },
  "plugins": [
    "./plugins/paginator_pub.coffee",
    "./plugins/paginator_blog.coffee"
  ],
  "require": {
    "moment": "moment",
    "typogr": "typogr",
    "fs": "fs"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator_pub": {
    "template": "publist.jade",
    "perPage": 99,
    "articles": "publications",
    "first": "publications/index.html"
  },
  "paginator_blog": {
    "template": "blog.jade",
    "perPage": 99,
    "articles": "blog",
    "first": "blog/index.html"
  }
}
