doctype html
block vars
	- var bodyclass = null;
html(lang='en')
	head
		block head
			meta(charset='utf-8')
			meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
			meta(name='viewport', content='width=device-width')
			title
				block title
			link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Cabin:400,700')
			link(rel='stylesheet', href='/style.css')
			script(src='/script.js')
	body
		block body

			header.header
				h1 Georg Nebehay
				a(href='https://twitter.com/gnebehay', target='_blank'): img(src='/img/Twitter_logo_blue.png', alt='Follow me on twitter' width='46')
				a(href='https://github.com/gnebehay', target='_blank'): img(src='/img/Octocat.png', width='46', alt='View my GitHub page')
				a(href='https://plus.google.com/+GeorgNebehay', target='_blank'): img(src='/img/Red-signin_Short_base_44dp.png', alt='Contact me on Google+', width='46')
				script.
					insert_mail_address();
			noscript Sorry, you need Javascript on to email me.

			div.navigation
				a(class='menuitem', href='/') Home
				a(class='menuitem', href='/tld/') OpenTLD
				a(class='menuitem', href='/publications/') Publications
				a(class='menuitem', href='/blog/') Blog

			div.content
				block content

			block footer

